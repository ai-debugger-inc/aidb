{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "FastAPI: Debug Server",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "app:app",
        "--host",
        "127.0.0.1",
        "--port",
        "8000",
        "--log-level",
        "info"
      ],
      "jinja": false,
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "FastAPI: Debug View",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "app:app",
        "--host",
        "127.0.0.1",
        "--port",
        "8001",
        "--log-level",
        "info"
      ],
      "jinja": false,
      "console": "integratedTerminal",
      "justMyCode": false
    }
  ]
}
